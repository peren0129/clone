<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<typeAliases>
		<typeAlias type="qna.model.QnaBean" alias="MyQna" />
		<typeAlias type="qna.model.QcommentBean" alias="MyComment" />
		<typeAlias type="notice.model.NoticeBean" alias="MyNotice" />
		<typeAlias type="member.model.userBean" alias="MyUser" />
		<typeAlias alias="MyOrder" type="order.model.OrderBean"/>
	</typeAliases>

	<mappers>
		<mapper resource="mybatis/qna.xml" />
		<mapper resource="mybatis/notice.xml" />
		<mapper resource="mybatis/user.xml" />
	</mappers>
</configuration>

